    "import pandas as pd\n",
    "import os\n",
    "\n",
    "csvs = [f for f in os.listdir() if f.endswith(\".csv\")]\n",
    "dfs = [pd.read_csv(csv) for csv in csvs]\n",
    "final = pd.concat(dfs, ignore_index=True)\n",
    "final = final.groupby(\"product_id\").mean().reset_index()\n",
    "final"
   ]
